<header class="site-nav">
  <div class="nav-content">
    <div class="logo">
      <a href="<%- url_for(theme.menu["home"]) %>"><%= theme.logo %></a>
    </div>
    <nav class="navbar">
      <ul>
        <% for(name in theme.menu) { %>
        <li class="menu-item">
          <a href="<%- url_for(theme.menu[name]) %>" class="menu-item-link"><%= __('menu.' + name) %></a>
        </li>
        <% } %>
      </ul>
    </nav>
  </div>
</header>